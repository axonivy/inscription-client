{
  "$schema" : "https://json-schema.axonivy.com/process/11.1.19/schema.json",
  "id" : "16C70B87DCB65433",
  "config" : {
    "data" : "screenshot.project.Invoice"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "WaitEvent",
      "name" : "read address",
      "config" : {
        "timeoutConfig" : {
          "duration" : "'2H30M'",
          "action" : "destroyTask"
        },
        "task" : {
          "name" : "<%=ivy.cms.co(\"/Task/Approval\")%>",
          "description" : "<%=ivy.cms.co(\"/Task/Approval/Desc\")%>",
          "category" : "Finance/Invoices/<%=in.invoiceType%>"
        },
        "eventBeanClass" : "ch.ivyteam.ivy.process.intermediateevent.beans.FileIntermediateEventBean",
        "eventBeanConfig" : [
          "/shared/company/addresses",
          "customerId"
        ],
        "eventIdConfig" : "in.customerId"
      },
      "visual" : {
        "at" : { "x" : 192, "y" : 64 },
        "labelOffset" : { "x" : 17, "y" : 36 }
      },
      "connect" : [
        { "id" : "f9", "to" : "f8" }
      ]
    }, {
      "id" : "f2",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 448, "y" : 64 }
      }
    }, {
      "id" : "f4",
      "type" : "RequestStart",
      "name" : "start.ivp",
      "config" : {
        "signature" : "start"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 64 }
      },
      "connect" : [
        { "id" : "f5", "to" : "f0" }
      ]
    }, {
      "id" : "f7",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 320, "y" : 192 }
      }
    }, {
      "id" : "f10",
      "type" : "ErrorStartEvent",
      "name" : "server not reachable",
      "visual" : {
        "at" : { "x" : 64, "y" : 320 },
        "labelOffset" : { "x" : 24, "y" : 37 }
      }
    }, {
      "id" : "f1",
      "type" : "ProgramStart",
      "name" : "erpUpdate",
      "config" : {
        "beanConfig" : [
          "interval=30",
          "path=/tmp/myErp"
        ],
        "outerBean" : "com.axonivy.wf.custom.ErpInvoice"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 192 },
        "labelOffset" : { "x" : 9, "y" : 38 },
        "description" : "file driven start to apply ERP updates"
      },
      "connect" : [
        { "id" : "f6", "to" : "f7" }
      ]
    }, {
      "id" : "f8",
      "type" : "ProgramInterface",
      "name" : "load ERP",
      "config" : {
        "beanConfig" : "path=in.creditStats",
        "outerBean" : "com.axonivy.wf.custom.ErpLoader",
        "timeout" : "10"
      },
      "visual" : {
        "at" : { "x" : 320, "y" : 64 }
      },
      "connect" : [
        { "id" : "f3", "to" : "f2" }
      ]
    }, {
      "id" : "f11",
      "type" : "WaitEvent",
      "name" : "ERP print",
      "config" : {
        "eventBeanClass" : "com.axonivy.wf.custom.ErpPrintJob",
        "output" : {
          "map" : {
            "out" : "in",
            "out.report" : "result as File"
          }
        },
        "eventBeanConfig" : [
          "interval=5",
          "path=/tmp/spooler/userReports"
        ],
        "eventIdConfig" : "\"erp-print\"+ivy.case.getId()"
      },
      "visual" : {
        "at" : { "x" : 192, "y" : 256 },
        "labelOffset" : { "x" : 9, "y" : 38 }
      },
      "connect" : [
        { "id" : "f14", "to" : "f13" }
      ]
    }, {
      "id" : "f12",
      "type" : "RequestStart",
      "name" : "printERP.ivp",
      "config" : {
        "signature" : "printERP"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 256 },
        "labelOffset" : { "x" : 16, "y" : 37 }
      },
      "connect" : [
        { "id" : "f15", "to" : "f11" }
      ]
    }, {
      "id" : "f13",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 320, "y" : 256 }
      }
    } ]
}