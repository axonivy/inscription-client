{
  "$schema" : "https://json-schema.axonivy.com/process/12.0.0/process.json",
  "id" : "16C70B87DCB65433",
  "config" : {
    "data" : "screenshot.project.Invoice"
  },
  "elements" : [ {
      "id" : "f0",
      "type" : "WaitEvent",
      "name" : "read address",
      "config" : {
        "javaClass" : "ch.ivyteam.ivy.process.intermediateevent.beans.FileIntermediateEventBean",
        "userConfig" : {
          "eventId" : "customerId",
          "directory" : "/shared/company/addresses"
        },
        "eventId" : "in.customerId",
        "timeout" : {
          "duration" : "'2H30M'",
          "action" : "DESTROY_TASK"
        },
        "task" : {
          "name" : "<%=ivy.cms.co(\"/Task/Approval\")%>",
          "description" : "<%=ivy.cms.co(\"/Task/Approval/Desc\")%>",
          "category" : "Finance/Invoices/<%=in.invoiceType%>"
        }
      },
      "visual" : {
        "at" : { "x" : 192, "y" : 64 },
        "labelOffset" : { "x" : 17, "y" : 36 }
      },
      "connect" : [
        { "id" : "f9", "to" : "f8" }
      ]
    }, {
      "id" : "f2",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 448, "y" : 64 }
      }
    }, {
      "id" : "f4",
      "type" : "RequestStart",
      "name" : "start.ivp",
      "config" : {
        "signature" : "start"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 64 }
      },
      "connect" : [
        { "id" : "f5", "to" : "f0" }
      ]
    }, {
      "id" : "f7",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 320, "y" : 192 }
      }
    }, {
      "id" : "f10",
      "type" : "ErrorStartEvent",
      "name" : "server not reachable",
      "visual" : {
        "at" : { "x" : 64, "y" : 320 },
        "labelOffset" : { "x" : 24, "y" : 37 }
      }
    }, {
      "id" : "f1",
      "type" : "ProgramStart",
      "name" : "erpUpdate",
      "config" : {
        "javaClass" : "com.axonivy.wf.custom.ErpInvoice",
        "userConfig" : {
          "path" : "/tmp/myErp",
          "interval" : "30"
        },
        "link" : "eventLink.ivp"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 192 },
        "labelOffset" : { "x" : 9, "y" : 38 },
        "description" : "file driven start to apply ERP updates"
      },
      "connect" : [
        { "id" : "f6", "to" : "f7" }
      ]
    }, {
      "id" : "f8",
      "type" : "ProgramInterface",
      "name" : "load ERP",
      "config" : {
        "javaClass" : "com.axonivy.wf.custom.ErpLoader",
        "userConfig" : {
          "path" : "in.creditStats"
        },
        "timeout" : {
          "seconds" : "10"
        }
      },
      "visual" : {
        "at" : { "x" : 320, "y" : 64 }
      },
      "connect" : [
        { "id" : "f3", "to" : "f2" }
      ]
    }, {
      "id" : "f11",
      "type" : "WaitEvent",
      "name" : "ERP print",
      "config" : {
        "javaClass" : "com.axonivy.wf.custom.ErpPrintJob",
        "userConfig" : {
          "path" : "/tmp/spooler/userReports",
          "interval" : "5"
        },
        "eventId" : "\"erp-print\"+ivy.case.getId()",
        "output" : {
          "map" : {
            "out" : "in",
            "out.report" : "result as File"
          }
        }
      },
      "visual" : {
        "at" : { "x" : 192, "y" : 256 },
        "labelOffset" : { "x" : 9, "y" : 38 }
      },
      "connect" : [
        { "id" : "f14", "to" : "f13" }
      ]
    }, {
      "id" : "f12",
      "type" : "RequestStart",
      "name" : "printERP.ivp",
      "config" : {
        "signature" : "printERP"
      },
      "visual" : {
        "at" : { "x" : 64, "y" : 256 },
        "labelOffset" : { "x" : 16, "y" : 37 }
      },
      "connect" : [
        { "id" : "f15", "to" : "f11" }
      ]
    }, {
      "id" : "f13",
      "type" : "TaskEnd",
      "visual" : {
        "at" : { "x" : 320, "y" : 256 }
      }
    } ]
}