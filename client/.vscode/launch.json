{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/mock.html",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    },
    {
      "name": "Run Inscription & Workflow GLSP Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "${workspaceFolder}/integrations/vscode/workspace",
        "--extensionDevelopmentPath=${workspaceFolder}/integrations/vscode/workflow-extension",
        "--extensionDevelopmentPath=${workspaceFolder}/integrations/vscode/inscription-extension"
      ],
      "outFiles": ["${workspaceRoot}/integrations/vscode/workflow-extension/lib/*.js"],
      "sourceMaps": true
    }
  ]
}

