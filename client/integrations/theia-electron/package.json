{
  "private": true,
  "name": "@axonivy/inscription-theia-electron",
  "version": "0.1.0",
  "main": "src-gen/frontend/electron-main.js",
  "theia": {
    "target": "electron",
    "frontend": {
      "config": {
        "applicationName": "Next Gen Inscription"
      }
    }
  },
  "dependencies": {
    "@theia/console": "1.32.0",
    "@theia/core": "1.32.0",
    "@theia/debug": "1.32.0",
    "@theia/editor": "1.32.0",
    "@theia/editor-preview": "1.32.0",
    "@theia/electron": "1.32.0",
    "@theia/file-search": "1.32.0",
    "@theia/filesystem": "1.32.0",
    "@theia/getting-started": "1.32.0",
    "@theia/keymaps": "1.32.0",
    "@theia/markers": "1.32.0",
    "@theia/messages": "1.32.0",
    "@theia/metrics": "1.32.0",
    "@theia/mini-browser": "1.32.0",
    "@theia/monaco": "1.32.0",
    "@theia/navigator": "1.32.0",
    "@theia/outline-view": "1.32.0",
    "@theia/output": "1.32.0",
    "@theia/plugin-dev": "1.32.0",
    "@theia/plugin-ext": "1.32.0",
    "@theia/plugin-ext-vscode": "1.32.0",
    "@theia/preferences": "1.32.0",
    "@theia/preview": "1.32.0",
    "@theia/process": "1.32.0",
    "@theia/property-view": "1.32.0",
    "@theia/task": "1.32.0",
    "@theia/terminal": "1.32.0",
    "@theia/toolbar": "1.32.0",
    "@theia/userstorage": "1.32.0",
    "@theia/variable-resolver": "1.32.0",
    "@theia/vsx-registry": "1.32.0",
    "@theia/workspace": "1.32.0",
    "@eclipse-glsp-examples/workflow-theia": "1.1.0-next.e0485dc.148",
    "@axonivy/inscription-theia-integration": "^0.1.0",
    "node-pty": "*",
    "nsfw": "*",
    "native-keymap": "*",
    "find-git-repositories": "*",
    "drivelist": "*"
  },
  "scripts": {
    "build": "yarn -s compile && yarn -s bundle",
    "bundle": "theia build --mode development",
    "clean": "theia clean",
    "compile": "tsc -b",
    "rebuild": "theia rebuild:electron --cacheRoot ../..",
    "start": "yarn -s rebuild && theia start --WF_GLSP=5007 --plugins=local-dir:../../plugins --",
    "start:debug": "yarn -s start --loglevel=debug --debug --remote-debugging-port=9222",
    "start:watch": "concurrently --kill-others -n tsc,bundle,run -c red,yellow,green \"tsc -b -w --preserveWatchOutput\" \"yarn -s watch:bundle\" \"yarn -s start\"",
    "watch": "concurrently --kill-others -n tsc,bundle -c red,blue \"tsc -b -w --preserveWatchOutput\" \"yarn -s watch:bundle\"",
    "watch:bundle": "theia build --watch --mode development",
    "watch:compile": "tsc -b -w"
  },
  "devDependencies": {
    "@theia/cli": "1.32.0",
    "electron": "^15.3.5"
  }
}