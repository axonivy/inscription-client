{
  "name": "@axonivy/editor-icons",
  "version": "0.1.0",
  "fontVersion": "1.0",
  "description": "The icon font for the Axon Ivy Process Editor",
  "private": true,
  "files": [
    "lib",
    "src",
    "src-gen"
  ],
  "main": "lib/ivy-icons",
  "types": "src-gen/ivy-icons",
  "module": "src-gen/ivy-icons",
  "devDependencies": {
    "fantasticon": "^2.0.0",
    "rimraf": "^4.1.1"
  },
  "scripts": {
    "clean": "rimraf src-gen/ivy-icons.*",
    "build": "fantasticon && tsc && yarn build:copyfiles",
    "build:clean": "yarn clean && yarn build",
    "build:copyfiles": "copyfiles -u 1 \"src-gen/**/*.css\" \"src-gen/**/*.ttf\" lib"
  }
}
