{
  "name": "@axonivy/editor-icons",
  "version": "11.1.0-next",
  "fontVersion": "1.0",
  "description": "The icon font for the Axon Ivy Editors",
  "private": false,
  "author": "Axon Ivy AG",
  "homepage": "https://developer.axonivy.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/axonivy/next-gen-inscription"
  },
  "files": [
    "lib",
    "src",
    "src-gen"
  ],
  "main": "lib/ivy-icons",
  "types": "src-gen/ivy-icons",
  "module": "src-gen/ivy-icons",
  "devDependencies": {
    "fantasticon": "^2.0.0",
    "rimraf": "^4.1.1"
  },
  "scripts": {
    "prepare": "yarn build",
    "clean": "rimraf src-gen/ivy-icons.*",
    "build": "fantasticon && tsc && yarn build:copyfiles",
    "build:clean": "yarn clean && yarn build",
    "build:copyfiles": "copyfiles -u 1 \"src-gen/**/*.css\" \"src-gen/**/*.ttf\" lib"
  }
}
